<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <record id="view_sigmap_sumilla_inherit_tree" model="ir.ui.view">
        <field name="name">sumilla.tree</field>
        <field name="model">sigmap.sumilla</field>
        <field name="inherit_id" ref="usuarios.view_sigmap_sumilla_tree"/>
        <field name="arch" type="xml">
            <field name="reparto_id" position="after">
                <field name="documento_id"/>
            </field>
        </field>
    </record>

    <record id="view_sigmap_sumilla_inherit_form" model="ir.ui.view">
        <field name="name">sumilla.form</field>
        <field name="model">sigmap.sumilla</field>
        <field name="inherit_id" ref="usuarios.view_sigmap_sumilla_form"/>
        <field name="arch" type="xml">
            <field name="responsable_delegado_id" position="after">
                <field name="documento_id"/>
            </field>
        </field>
    </record>

</odoo>

