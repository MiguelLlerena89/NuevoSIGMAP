<?xml version="1.0" encoding="utf-8"?>
<odoo>
  <data noupdate="1">
    <record id="documento_trafico_sequence_id" model="ir.sequence">
      <field name="name">Secuencia Documento Tráfico Marítimo</field>
      <field name="code">documento_trafico_maritimo_sequence_code</field>
      <field name="prefix">DOCTRAFM/%(year)s/%(month)s/</field>
      <field name="padding">5</field>
      <field name="number_next">1</field>
    </record>

    <record id="documento_trafico_maritimo_permiso_zarpe_nacional_sequence_id" model="ir.sequence">
      <field name="name">Secuencia Documento Permiso de Zarpe Nacional Nave</field>
      <field name="code">documento_trafico_maritimo_permiso_zarpe_code</field>
      <field name="prefix">DZAN-</field>
      <field name="suffix">-%(year)s</field>
      <field name="padding">7</field>
      <field name="number_next">1</field>
    </record>

    <record id="documento_trafico_maritimo_permiso_arribo_nacional_sequence_id" model="ir.sequence">
      <field name="name">Secuencia Documento Permiso de Arribo Nacional Nave</field>
      <field name="code">documento_trafico_maritimo_permiso_arribo_code</field>
      <field name="prefix">DARN-</field>
      <field name="suffix">-%(year)s</field>
      <field name="padding">7</field>
      <field name="number_next">1</field>
    </record>

    <record id="documento_trafico_maritimo_permiso_zarpe_internacional_sequence_id" model="ir.sequence">
      <field name="name">Secuencia Documento Permiso de Zarpe Internacional Nave</field>
      <field name="code">documento_trafico_maritimo_permiso_zarpe_internacional_code</field>
      <field name="prefix">DZAI-</field>
      <field name="suffix">-%(year)s</field>
      <field name="padding">7</field>
      <field name="number_next">1</field>
    </record>

    <record id="documento_trafico_maritimo_permiso_arribo_internacional_sequence_id" model="ir.sequence">
      <field name="name">Secuencia Documento Permiso de Arribo Internacional Nave</field>
      <field name="code">documento_trafico_maritimo_permiso_arribo_internacional_code</field>
      <field name="prefix">DARI-</field>
      <field name="suffix">-%(year)s</field>
      <field name="padding">7</field>
      <field name="number_next">1</field>
    </record>

  </data>
</odoo>